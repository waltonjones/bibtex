@article{Jana:2011ji,
	Affiliation = {Tata Institute of Fundamental Research, Colaba, Mumbai 400005, India.},
	Author = {Jana, Swadhin C and Girotra, Mukul and Ray, Krishanu},
	Date-Added = {2014-10-21 04:40:43 +0000},
